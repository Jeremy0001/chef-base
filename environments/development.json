{
  "name": "development",
  "description": "The Web Application Developer's vagrant Chef environment",
  "json_class": "Chef::Environment",
  "chef_type": "environment",
  "override_attributes": {
    "authorization": {
      "sudo": {
      "groups": ["admin"],
      "users": ["vagrant"],
      "passwordless": "true"
      }
    },
    "monit_enabled": false,
    "dbhosts": {
      "db_ip": ["127.0.0.1"],
      "webapp_ip": ["127.0.0.1"]
    },
    "w_haproxy": {
      "listen_port": "82",
      "backend_port": "81"
    },
    "varnish": {
      "listen_port": "80",
      "backend_port": "81"
    },
    "w_varnish": {
      "node_ipaddress_list": ["192.168.33.10"],
      "acl_purge": ["\"192.168.33.10\"/32"],
      "geoip":{
        "enabled":true,
        "s3": {
          "enabled": false,
        }
      },
      "multi_cookie_enabled": true,
      "listen_address": "192.168.33.10"
    },
    "apache": {
      "listen_ports": ["81"],
      "default_site_port": "81"
    },
    "w_apache": {
      "xdebug_enabled": true,
      "config_test_enabled": true,
      "deploy":{
        "enabled": false
      },
      "phpmyadmin": {
        "enabled": true
      }
    },
    "w_memcached": {
      "ips": ["127.0.0.1"]
    },
    "percona": {
      "cluster": {
        "wsrep_cluster_name":"vagrant_cluster",
        "cluster_ips": ["127.0.0.1"]
      },
      "server": {
        "key_buffer": "512M",
        "query_cache_size": "256M",
        "innodb_buffer_pool_size": "512M",
        "bind_address": "127.0.0.1"
      },
      "skip_passwords": false
    },
    "w_percona": {
      "xinetd_enabled": false
    }
  }
}
